{
  "name": "z402-client",
  "version": "0.1.0",
  "description": "Client SDK for Zero Trust x402 protocol",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@solana/web3.js": "^1.95.0",
    "@solana/spl-token": "^0.4.6",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.6.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --tsconfig tsconfig.json && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "solana",
    "x402",
    "payments",
    "express",
    "middleware",
    "zero-trust",
    "z402"
  ],
  "author": "chickstudio",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chickstudi0/z402.git"
  },
  "license": "MIT"
}
