{
  "name": "z402-storage-sqlite",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "z402": "file:../core",
    "better-sqlite3": "^9.4.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "@types/better-sqlite3": "^7.6.13"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --tsconfig tsconfig.json && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "repository": { "type":"git", "url":"git+https://github.com/chickstudi0/z402.git" },
  "license": "MIT"
}
