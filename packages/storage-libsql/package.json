{
  "name": "z402-storage-libsql",
  "version": "0.1.0",
  "description": "LibSQL storage adapter for z402",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "z402": "file:../core"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --tsconfig tsconfig.json && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chickstudi0/z402.git"
  },
  "author": "chickstudio",
  "license": "MIT"
}
